{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "no-console": "off",
        "no-use-before-define": "warn",
        "no-undef": "warn",
        "no-unused-vars": "warn",
        "prefer-destructuring": "off",
        "operator-linebreak": "off",
        "comma-dangle": "off"
    }
}
